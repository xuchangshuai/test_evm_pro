pragma solidity >=0.7.0 <0.9.0;

contract TestAddModAndMulMod {

    function addmod(uint256 a, uint256 b, uint256 c) public pure returns (uint256){
        return addmod(a, b, c);
    }

    function mulmod(uint256 a, uint256 b, uint256 c) public pure returns (uint256){
        return mulmod(a, b, c);
    }
}